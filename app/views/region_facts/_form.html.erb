<%= form_for @region_fact do |f| %>
<p>
  <%= f.label :fact %>
  <%= f.text_field :fact, :autofocus => true, :class => 'form-control', :placeholder => "This is Sydney's wiki page"  %>
    <%= f.label :link %>
  <%= f.text_field :link, :class => 'form-control', :placeholder => "http://en.wikipedia.org/wiki/Sydney" %>
</p>

<%= f.submit "Save and continue", :class => 'btn btn-primary' %>

<% end %>